{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fleximple-blocks/tabs",
  "title": "Tabs",
  "category": "fleximple-blocks",
  "attributes": {
    "count": {
      "type": "number",
      "default": 3
    },
    "tabsData": {
      "type": "array",
      "default": [
        { "label": "Tab 1" },
        { "label": "Tab 2" },
        { "label": "Tab 3" }
      ]
    },
    "tabsAlignment": {
      "type": "string",
      "default": "left"
    }
  }
}
