{
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"bracketSpacing": true,
	"trailingComma": "es5",
	"jsxSingleQuote": true,
	"jsxBracketSameLine": false,
